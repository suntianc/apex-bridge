{
  "name": "apex-bridge",
  "version": "1.0.1",
  "description": "Apex Bridge - AI Protocol Server with tool calling, variable resolution, and plugin management",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:memory-runtime": "ts-node tests/runtime/memory-service-runtime-test.ts",
    "test:websocket": "node tests/websocket-test.js",
    "test:api": "bash tests/api-test.sh",
    "migrate:plugins": "ts-node scripts/migrate-plugins.ts",
    "migrate:plugins:dry-run": "ts-node scripts/migrate-plugins.ts --dry-run",
    "migrate:skills:to-abp": "ts-node scripts/migrate-skills-to-abp.ts",
    "migrate:skills:to-abp:dry-run": "ts-node scripts/migrate-skills-to-abp.ts --dry-run",
    "migrate:skills:to-claude": "ts-node scripts/migrate-skills-to-claude-package.ts",
    "validate:skills:claude": "ts-node scripts/migrate-skills-to-claude-package.ts --validate",
    "validate:skills": "ts-node scripts/validate-skills.ts",
    "validate:skills:strict": "ts-node scripts/validate-skills.ts --strict",
    "ab-test:skills": "ts-node scripts/ab-test-skills.ts",
    "docs:check-badges": "node ../scripts/check-badges.js",
    "docs:lint": "markdownlint *.md",
    "docs:format": "prettier --write *.md",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "build:admin": "cd admin && npm run build",
    "build:all": "npm run build && npm run build:admin"
  },
  "keywords": [
    "ai",
    "protocol",
    "llm",
    "chatbot",
    "tools",
    "skills",
    "rag",
    "apexbridge"
  ],
  "author": "ApexBridge Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/suntianc/apex-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/suntianc/apex-bridge/issues"
  },
  "homepage": "https://github.com/suntianc/apex-bridge#readme",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "gray-matter": "^4.0.3",
    "helmet": "^7.1.0",
    "hnswlib-node": "^3.0.0",
    "node-schedule": "^2.1.1",
    "redis": "^4.6.12",
    "tmp": "^0.2.5",
    "typescript": "^5.0.0",
    "vcp-intellicore-rag": "^1.0.2",
    "vm2": "^3.9.19",
    "winston": "^3.11.0",
    "ws": "^8.17.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/node-schedule": "^2.1.8",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript-eslint": "^8.46.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
