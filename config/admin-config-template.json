{
  "setup_completed": true,
  "api": {
    "host": "0.0.0.0",
    "port": 3000,
    "cors": {
      "origin": "*",
      "credentials": true
    }
  },
  "llm": {
    "providers": [],
    "defaultProvider": "openai",
    "timeout": 30000,
    "maxRetries": 3
  },
  "auth": {
    "enabled": true,
    "apiKey": "",
    "jwtSecret": "your-secret-key",
    "jwtExpiresIn": "24h"
  },
  "performance": {
    "workerPoolSize": 4,
    "requestTimeout": 60000,
    "maxRequestSize": "50mb"
  },
  "redis": {
    "enabled": false,
    "host": "localhost",
    "port": 6379,
    "db": 0,
    "keyPrefix": "apex_bridge:",
    "connectTimeout": 10000,
    "lazyConnect": true,
    "maxRetriesPerRequest": 3,
    "retryDelayOnFailover": 100
  },
  "security": {
    "rateLimit": {
      "enabled": true,
      "windowMs": 900000,
      "max": 1000,
      "message": "请求过于频繁，请稍后再试",
      "standardHeaders": true,
      "legacyHeaders": false,
      "trustProxy": false
    }
  },
  "ace": {
    "enabled": true,
    "orchestration": {
      "enabled": true,
      "mode": "full"
    },
    "layers": {
      "l1": {
        "enabled": true,
        "constitutionPath": "./config/constitution.md",
        "modelSource": "sqlite"
      },
      "l2": {
        "enabled": true,
        "modelSource": "sqlite"
      },
      "l3": {
        "enabled": true,
        "modelSource": "sqlite"
      },
      "l4": {
        "enabled": true,
        "modelSource": "sqlite"
      },
      "l5": {
        "enabled": true,
        "modelSource": "sqlite",
        "fallbackToEvolution": true
      },
      "l6": {
        "enabled": true,
        "useLLM": false
      }
    },
    "memory": {
      "provider": "lancedb",
      "vectorDbPath": "./.data",
      "collectionPrefix": "apex_bridge"
    },
    "optimization": {
      "fastTrackSimpleTasks": true,
      "l5ScratchpadCompression": true,
      "l6NonLLMExecution": true
    },
    "skills": {
      "autoCleanupEnabled": true,
      "cleanupTimeoutMs": 300000,
      "maxActiveSkills": 50
    },
    "localImplementation": {
      "enabled": true,
      "aceCore": {
        "reflectionCycleInterval": 60000,
        "maxSessionAge": 86400000
      },
      "useEventBus": true,
      "useLLMManager": true,
      "useSQLiteConfig": true
    }
  }
}
